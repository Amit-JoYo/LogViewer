<Window x:Class="SharpTail.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:controls="clr-namespace:SharpTail.Ui.Controls" xmlns:ide="clr-namespace:SharpTail"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance ide:MainWindowViewModel, IsDesignTimeCreatable=True}"
        Title="SharpTail" Height="350" Width="525">
    <Grid>
        <controls:LogViewerControl
            DataContext="{Binding LogViewModel}" />
    </Grid>
</Window>